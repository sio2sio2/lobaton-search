{
  "name": "@lobaton/search",
  "description": "Búsqueda de centros para Lobatón",
  "author": "José Miguel Sánchez Alés <sio2sio2+github@gmail.com>",
  "version": "0.1.1",
  "main": "dist/search-src.js",
  "scripts": {
    "build": "webpack --env.output=min --progress",
    "build:src": "webpack --env.output=src --progress",
    "build:bundle": "webpack --env.output=bundle --progress"
  },
  "license": "MIT",
  "keywords": [
    "lobaton",
    "fuse",
    "search"
  ],
  "browserslist": {
    "production": [
      "cover 95% in ES"
    ],
    "oneplus": [
      "> 1% in ES"
    ],
    "ie10": [
      "> .25% in ES",
      "ie >= 10"
    ],
    "relax": [
      "cover 80% in ES"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sio2sio2/lobaton-search.git"
  },
  "homepage": "https://github.com/sio2sio2/lobaton-search#readme",
  "bugs": {
    "url": "https://github.com/sio2sio2/lobaton-search/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "core-js": "^3.2.1",
    "fuse.js": "^3.4.5",
    "leaflet-search": "^2.9.8"
  }
}
